{
  "extends": "eslint:recommended",
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "semi": [
      "error",
      "always"
    ],
    "quotes": "off",
    "no-console": "off",
    "no-unused-vars": "off",
    "no-unreachable": "off",
    "no-redeclare": "warn"
  }
}