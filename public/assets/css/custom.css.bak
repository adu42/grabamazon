    padding: 0;
}
/*
.col-sm-12.col-md-12.col-lg-8.content-wrapper{
  padding: 0 0 30px 18px;
}
*/

@media (min-width: 1200px) {
    li.max-hide{display: none;}
    body > .container{
        min-height: 699px;
    }
}
@media (max-width: 1199px) {
    .article-side{
        margin-top: 20px;
        clear: both;
        padding-right: 0px;
        padding-left: 0px;
    }
    li.max-hide{display: inline-block;}
    .sidebar-module.min-hide {
        display: none;
    }
    body > .container{
        min-height: 699px;
    }
}

@media screen and (max-width: 768px){
    .content-wrapper{
        float: none;
    }
    .article-side{
        margin-top: 20px;
        clear: both;
        padding-right: 0px;
        padding-left: 0px;
    }
    .min-hide{display: none;}
    .navbar-toggle .icon-bar{
        background-color: #000;;
    }
    body > .container{
        min-height: 400px;
    }
    .carousel.slide{
        display: none;
    }
    .carousel.slide .col-md-4{
        margin-bottom: 20px;
    }

}


@media screen and (max-width: 480px){
    .col-sm-12.col-md-12.col-lg-8.content-wrapper{
        padding: 0;
    }
    .carousel.slide{
        display: none;
    }

}

